<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="signInPane" fx:id="signInPane" prefHeight="600.0" prefWidth="720.0" styleClass="rootPane" stylesheets="@application.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.SignInController">
   <children>
      <AnchorPane id="signInCenterPane" fx:id="signInCenterPane" layoutX="165.0" layoutY="55.0" prefHeight="490.0" prefWidth="390.0" styleClass="centerPane">
         <children>
            <TextField fx:id="signInEmailTf" focusTraversable="false" layoutX="89.0" layoutY="152.0" prefHeight="32.0" prefWidth="229.0" promptText="email@example.com" styleClass="textField">
               <font>
                  <Font size="16.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor>
            </TextField>
            <Button id="signInSignInBt" fx:id="signInSignInBt" focusTraversable="false" layoutX="89.0" layoutY="312.0" mnemonicParsing="false" onMouseClicked="#signInButtonClick" prefHeight="37.0" prefWidth="229.0" text="SignIn" textAlignment="CENTER">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Label layoutX="89.0" layoutY="125.0" text="Email">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label fx:id="SignInSignUpLb" layoutX="88.0" layoutY="421.0" onMouseClicked="#signUpLableClick" text="SignUp" textFill="#007c5d" underline="true">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <Button id="signInSkipBt" fx:id="signInSkipBt" focusTraversable="false" layoutX="280.0" layoutY="418.0" mnemonicParsing="false" onMouseClicked="#skipButtonClick" text="skip" />
            <Label layoutX="89.0" layoutY="209.0" text="Password">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="139.0" layoutY="45.0" text="OKKHOR">
               <font>
                  <Font name="System Bold" size="30.0" />
               </font>
            </Label>
            <PasswordField fx:id="signInPasswordTf" focusTraversable="false" layoutX="90.0" layoutY="236.0" prefHeight="37.0" prefWidth="229.0" promptText="3&lt;password&lt;8" styleClass="textField">
               <font>
                  <Font size="16.0" />
               </font>
            </PasswordField>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
